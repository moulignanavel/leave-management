{
  "name": "leave-management-system",
  "version": "1.0.0",
  "description": "Leave Management System using MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "install-all": "npm install && cd frontend && npm install",
    "seed": "node backend/seedUsers.js"
  },
  "keywords": [
    "leave",
    "management",
    "mern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.4.0",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^7.1.0",
    "mongoose": "^7.3.1",
    "nodemailer": "^8.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
